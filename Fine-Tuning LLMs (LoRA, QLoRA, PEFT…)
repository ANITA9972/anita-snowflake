    {
      "cell_type": "code",
      "source": [
        "\n",
        "def ask(question):\n",
        "    prompt = f\"<|user|>{question}<|end|><|assistant|>\"\n",
        "    inputs = tokenizer(prompt, return_tensors=\"pt\").to(\"cuda\")\n",
        "    outputs = model.generate(**inputs, max_new_tokens=100, temperature=0.7)\n",
        "    return tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "\n",
        "# Test before vs after\n",
        "print(ask(\"What are diabetes symptoms?\"))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "7j4jQZIDFsv1",
        "outputId": "c493de66-7a9f-4b74-a75d-ffbd8df89630"
      },
      "execution_count": 21,
